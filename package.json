{
  "name": "resume-edge-ai",
  "displayName": "Resume edge ai",
  "version": "0.0.1",
  "description": "Instantly tailor your resume to any job description using AI. Maximize your chances with ResumeEdge.ai.",
  "author": "Nikhil Kumar <nikhil.nklp@gmail.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "plasmo": {
    "content_scripts": [
      {
        "matches": [
          "*://www.linkedin.com/jobs/*"
        ],
        "js": [
          "contents/read-jd.content.ts"
        ]
      }
    ]
  },
  "host_permissions": [
    "https://www.linkedin.com/*"
  ],
  "dependencies": {
    "@plasmohq/storage": "^1.15.0",
    "pdfjs-dist": "^5.3.93",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ]
  }
}
